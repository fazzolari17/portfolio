{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.27.2",
    "formik": "^2.2.9",
    "history": "^5.3.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-loading-spin": "^2.1.9",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.6.2",
    "react-scripts": "5.0.1",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "REACT_APP_ENVIRONMENT=development react-scripts start",
    "dev:prod": "REACT_APP_ENVIRONMENT=production_server react-scripts start",
    "build": "react-scripts build && echo '/* /index.html  200' | cat >build/_redirects ",
    "test": "ENV=\"test\" react-scripts test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "testlog": "ENV=\"test\" react-scripts test -- --silent=false --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js src/**/*.jsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "jest": "^27.5.1",
    "prettier": "^2.8.3",
    "react-test-renderer": "^18.2.0"
  }
}
